#: doc/tin.5:2773
#, no-wrap
msgid "B<n>"
msgstr ""

#. type: Plain text
#: doc/tin.5:2776
msgid "no local postings are allowed, only remote ones"
msgstr ""

#. type: TP
#: doc/tin.5:2776
#, no-wrap
msgid "B<m>"
msgstr ""

#. type: Plain text
#: doc/tin.5:2779
